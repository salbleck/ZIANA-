{
  "version": 2,                
  "builds": [
    {
      "src": "onlinequiz/wsgi.py",  
      "use": "@vercel/python"       
    }
  ],
  "routes": [
    {
      "src": "/(.*)",              
      "dest": "onlinequiz/wsgi.py"  
    }
  ],
  "env": {                           
    "DJANGO_SECRET_KEY": "isi_secret_key",  
    "DEBUG": "False",                       
    "DATABASE_URL": "isi_url_database"     
  }
}
